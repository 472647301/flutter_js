    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_js",
            "request": "launch",
            "type": "dart",
            "args": ["--verbose"],
            "program": "example/lib/main.dart"
        },
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": ["--verbose"]
        },
        {
            "name": "packages",
            "cwd": "packages",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "test-with-flutterjs",
            "type": "dart",
            "program": "test/flutter_js_test.dart",
            "env": {
                "PATH": "${env:Path};${workspaceFolder}\\example\\build\\windows\\runner\\Debug"
            },
            "request": "launch"
        }
    ]
}